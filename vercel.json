{
  "version": 2,
  "builds": [
    {
      "src": "my-vite-react-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "my-vite-react-app/dist",
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/my-vite-react-app/dist/assets/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/my-vite-react-app/dist/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/my-vite-react-app/dist/index.html"
    }
  ]
}
